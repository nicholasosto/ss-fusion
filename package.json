{
  "name": "@trembus/ss-fusion",
  "version": "2.3.0-alpha.0",
  "description": "A modern, reactive UI component library for Roblox built with TypeScript and Fusion",
  "main": "out/init.luau",
  "types": "index.d.ts",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholasosto/ss-fusion.git"
  },
  "homepage": "https://github.com/nicholasosto/ss-fusion#readme",
  "bugs": {
    "url": "https://github.com/nicholasosto/ss-fusion/issues"
  },
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": "./out/init.luau",
    "./atoms": "./out/atoms/init.luau",
    "./atoms/*": "./out/atoms/*",
    "./molecules": "./out/molecules/init.luau",
    "./molecules/*": "./out/molecules/*",
    "./organisms": "./out/organisms/init.luau",
    "./organisms/*": "./out/organisms/*",
    "./types": "./out/types/init.luau",
    "./types/*": "./out/types/*",
    "./layout/*": "./out/layout/*",
    "./utils/*": "./out/utils/*",
    "./examples/*": "./out/examples/*"
  },
  "typesVersions": {
    "*": {
      "atoms": [
        "out/atoms/index.d.ts"
      ],
      "atoms/*": [
        "out/atoms/*"
      ],
      "molecules": [
        "out/molecules/index.d.ts"
      ],
      "molecules/*": [
        "out/molecules/*"
      ],
      "organisms": [
        "out/organisms/index.d.ts"
      ],
      "organisms/*": [
        "out/organisms/*"
      ],
      "types": [
        "out/types/index.d.ts"
      ],
      "types/*": [
        "out/types/*"
      ],
      "layout/*": [
        "out/layout/*"
      ],
      "utils/*": [
        "out/utils/*"
      ],
      "examples/*": [
        "out/examples/*"
      ],
      "*": [
        "out/*"
      ]
    }
  },
  "files": [
    "out",
    "index.d.ts",
    "VERSIONS.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "3.0.0-types.0",
    "@rbxts/types": "^1.0.877",
    "@types/node": "^24.3.0",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@rbxts/fusion": "^0.2.0",
    "@rbxts/services": "^1.5.5"
  },
  "packageManager": "pnpm@10.13.1"
}
